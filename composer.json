{
  "name": "arraypress/wp-hash-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for hashing, password security, data integrity, and verification",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-hash-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\HashUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-hash-utils/issues",
    "source": "https://github.com/arraypress/wp-hash-utils"
  },
  "keywords": [
    "wordpress",
    "hash",
    "hashing",
    "password",
    "security",
    "integrity",
    "verification",
    "nonce",
    "hmac",
    "utilities",
    "arraypress"
  ]
}